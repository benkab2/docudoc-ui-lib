    "autoprefixer": "^10.4.16",
    "babel-jest": "^29.7.0",
    "chromatic": "^10.9.6",
    "classnames": "^2.3.2",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
