{
  "name": "@docudoc/ui-library",
  "description": "DocuDoc UI Library By DevXcellence",
  "version": "0.0.1",
  "files": [
    "dist/**/*"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "jest",
    "lint": "eslint src/**/*.{ts,tsx} && echo \"✅ Linting Successful!\" || echo \"❌ Linting Failed!\"",
    "lint:fix": "eslint --fix 'src/**/*.{ts,tsx}'",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc",
    "tsc:check": "tsc",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "rm -rf dist",
    "build": "npm run build:js && npm run build:types",
    "build:js": "rollup -c --bundleConfigAsCjs",
    "build:types": "tsc --emitDeclarationOnly --outDir dist",
    "validate": "npm run test && npm run format && npm run lint && npm run tsc:check && npm run clean && npm run build"
  },
  "author": {
    "name": "DevXcellence"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.19",
